{
    "name": "ozzythegiant/electro",
    "description": "A sample electric bill tracking application",
    "type": "project",
    "require": {
		"php": "^8.2.0",
        "slim/slim": "^4.12.0",
        "slim/psr7": "^1.6.0",
        "monolog/monolog": "^1.24",
        "illuminate/database": "^10.15.0",
        "vlucas/phpdotenv": "^5.5.0"
	},
    "require-dev": {
        "mockery/mockery": "^1.6.4",
        "phpunit/phpunit": "10.2.6"
    },
    "authors": [
        {
            "name": "Oziel Perez",
            "email": "OZLperez11@gmail.com"
        }
	],
	"config": {
        "optimize-autoloader": true,
        "preferred-install": "build",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
	"autoload": {
		"psr-4": {
			"App\\":"laravel/app",
			"Electro\\models\\":["slim/models"],
			"Electro\\exceptions\\":["slim/exceptions"],
			"Electro\\controllers\\":["slim/controllers"],
			"Electro\\middleware\\":["slim/middleware"],
			"Electro\\services\\":["slim/services"]
		}
	},
	"autoload-dev": {
        "psr-4": {
            "Tests\\": "laravel/tests/"
        }
    },
    "minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"test":"./vendor/bin/phpunit --bootstrap vendor/autoload.php slim/tests"
    }
}
